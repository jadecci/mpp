### Configurations
# cv_seed: seed for RepeatedStratifiedKFold random_state option
# n_folds: number of folds for KFold cross-validation
# n_repeats: number of times to repeat cross-validation
# n_repeats_perm: number of times to repeat cross-validation for permutation tests
# n_alphas: number of alphas along the regularization path for ElasticNetCV
# features_criteria: criteria for feature selection (r or cod)

[DEFAULT]
cv_seed = 42
n_folds = 10
n_repeats = 10
n_repeats_perm = 100
n_alphas = 10
features_criteria = r

